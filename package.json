{
	"name": "kha",
	"displayName": "Kha",
	"description": "Kha support",
	"version": "17.9.1",
	"publisher": "kodetech",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension",
	"contributes": {
		"commands": [{
			"command": "kha.init",
			"title": "Init Kha Project"
		},
		{
			"command": "kha.compile",
			"title": "Compile Kha Project"
		}],
		"configuration": {
            "type": "object",
            "title": "Kha configuration",
            "properties": {
                "kha.khaPath": {
					"type": "string",
					"default": "",
					"description": "Points to a Kha directory."
                },
				"kha.ffmpeg": {
					"type": "string",
					"default": "",
					"description": "Path to ffmpeg for extended audio and video format support."
				}
            }
        }
	},
	"devDependencies": {
		"vscode": "0.10.x"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Kode/vscode-kha"
	}
}
